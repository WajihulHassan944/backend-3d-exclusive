{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs20.x"
    },
    "server.js": {
      "runtime": "nodejs20.x"
    }
  },
  "routes": [
    { "src": "/(.*)", "dest": "server.js" }
  ],
  "crons": [
    {
      "path": "/api/coupons/expire",
      "schedule": "0 0 * * *"
    }
  ]
}
