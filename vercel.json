{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/server.js" }
  ],
  "crons": [
    {
      "path": "/api/coupons/expire",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/blogs/publish-scheduled",
      "schedule": "0 0 * * *"
    }
  ]
}
